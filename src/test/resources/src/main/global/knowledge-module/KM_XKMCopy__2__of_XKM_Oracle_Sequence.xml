public class XKM_Oracle_Sequence extends GroovyComponentGeneratorDelegateBase {
  protected selectExprList;

  private mBinding = null;
  public void setBinding(Binding binding) {
    super.setBinding(binding);
    mBinding = binding;
    Map variables = binding.getVariables();
	selectExprList = {
	  def result = new ArrayList<ISelectItem>()
	  def attrs = component.getAttributes();
	  ArrayExpression expr;
	  ISequence seq;
	  String accessName;
	  String fullAccessName;
	  IContext context = physicalNode.getExecutionUnit().getContext();
	  seq = (ISequence) component.getBoundObject();
	  def refType = component.getRefType();
	  boolean first = true;
	  for (attr in attrs) {
	    if (seq.getSequenceType().equals("NATIVE")) {
	      // Special sequence SDK access name call
	      // This is necessary because the native sequence may be found in a different
	      // location than that of the sequence object itself.
	      // For an ODI sequence, the sequence object does not actually have a location.
	      //accessName = SequenceComponent.getSequenceAccessName(seq, refType, context, physicalNode.getAdapter());  
	      accessName = SequenceComponent.getGeneratedSequenceRef(seq, refType, context);  // Use 11g-compatible sequence reference
	      if (first) {
	        fullAccessName = accessName + "_NEXTVAL" // Oracle specific
	      } else {
	        fullAccessName = accessName + "_CURRVAL" // Oracle specific
	      }
	      result.add(new StringExpression(fullAccessName, attr));
	    }
	    first = false;
	  }
	  return result
	}
	
	 mBinding.setVariable("selectExprList", selectExprList);
	}
	
	public XKM_Oracle_Sequence(){
	}
	
	AbstractSyntaxTree SELECT() {
	  def result = new SqlQuery();
	  result.setIsPassive(true);
	  result.addFrom(new FromClause(new TableReference(new Table("dual")), "DUMMY_TABLE")) // "dual" is Oracle specific
	  List<ISelectItem> selectList = selectExprList.call()
	  result.setSelectList(selectList)
	   
	  return result
	}
}

