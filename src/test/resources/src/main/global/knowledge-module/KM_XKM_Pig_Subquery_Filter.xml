	        MapConnectorPoint driverConnectorPoint = subqFilterComp.getDriverConnectorPoint();
	        List<MapAttribute> componentAttributes = component.getAttributes();
        	for (MapAttribute attribute : componentAttributes) {
        		oracle.odi.domain.mapping.expression.MapExpression driverExpr = attribute.getExpressionMap().get(driverConnectorPoint);
        		if (driverExpr != null) {
        			driverExprs.add(driverExpr);
        		}
        	}
            
            
            MapConnectorPoint subqueryConnectorPoint = subqFilterComp.getSubqueryFilterConnectorPoint();
            for (MapAttribute attribute : componentAttributes) {
                oracle.odi.domain.mapping.expression.MapExpression subqueryExpr = attribute.getExpressionMap().get(subqueryConnectorPoint);
                subqueryExprs.add(subqueryExpr);
            }
	        
			subqueryFilterCmd.setOutputExprs(driverExprs);
			subqueryFilterCmd.setSubqExprs(subqueryExprs);
			
		    return subqueryFilterScript;
		}
	}	

