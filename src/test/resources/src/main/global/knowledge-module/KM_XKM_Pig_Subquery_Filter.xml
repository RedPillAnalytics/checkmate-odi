	        subqueryFilterScript.addChild(subqueryFilterCmd);
	        
	        PigCommand driverCommand = pigScripts.get(0).getLastCommand();
	        PigCommand subqueryCommand = pigScripts.get(1).getLastCommand();
	        
	        
	        SubqueryFilterComponent subqFilterComp = (SubqueryFilterComponent) component;
