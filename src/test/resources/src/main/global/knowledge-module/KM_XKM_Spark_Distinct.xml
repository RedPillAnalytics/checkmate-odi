	 @GeneratorDelegateClass(componentType=AggregateComponent.COMPONENT_TYPE_NAME, descriptionKey="GeneratorDelegateClass.SparkDistinctCompKM")
     public class XKM_Spark_Distinct extends SparkKM {
	    	@ASTProducerMethod(processingType=ProcessingType.SOURCE,producedASTType=SparkScript.class, styleKey="ProcessingStyleKey.SparkDistinct.SELECT")
            SparkScript SELECT(SparkScript inputScript) {

            supportDataFrames('XKM Spark Distinct', true);

            def sparkDistinctCmd = new SparkDistinctCmd(physicalNode.getTemplateUtils(), physicalNode);
        
            List<SparkScript> inputScripts = new ArrayList<SparkScript>(1);
            inputScripts.add(inputScript);
            SparkScript distinctScript = new SparkScript(physicalNode.getTemplateUtils(), inputScripts);
            distinctScript.addChild(sparkDistinctCmd);
            
              List<oracle.odi.domain.mapping.expression.MapExpression> targetAttributeExprs = new ArrayList<oracle.odi.domain.mapping.expression.MapExpression>();
              List<String> targetAttributeNames = new ArrayList<String>();
        
              List<MapAttribute> attrs = component.getAttributes();
              for (oracle.odi.domain.mapping.MapAttribute mapAttribute : attrs)
              {
                targetAttributeExprs.add(mapAttribute.getExpression()); 
                targetAttributeNames.add(mapAttribute.getName());
              }
              sparkDistinctCmd.setTargetAttrExprs(targetAttributeExprs);
              sparkDistinctCmd.setTargetAttrNames(targetAttributeNames);
        
              // Processes WINDOW_AGGREGATION, WINDOWING, WINDOW_LENGTH, and SLIDING_INTERVAL options
              sparkDistinctCmd.setWindowingValues(sparkDistinctCmd);
              
              sparkDistinctCmd.setUseSQLExpressions(getOptionValue("SQL_EXPRESSIONS").equalsIgnoreCase("true") ? true : false)
              
              return distinctScript;
            }
        }
    ]]></Field>
