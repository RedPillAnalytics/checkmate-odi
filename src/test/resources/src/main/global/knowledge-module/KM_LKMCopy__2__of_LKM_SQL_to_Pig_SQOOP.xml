	<Field name="DelegateScript" type="java.lang.String"><![CDATA[
      
	public class LKM_SQL_to_Pig_SQOOP extends LKM_File_to_Pig {
		protected mBinding = null;

		public void setBinding(Binding binding) {
            		super.setBinding(binding);
            		mBinding = binding;
		}

	
 		AbstractSyntaxTree EXTRACT_TAP(SqlQuery sqlQuery) throws AdapterException,
                                                   	oracle.odi.domain.mapping.exception.MappingException,
                                                        GenerationException {
			PigScript pigScript = super.EXTRACT_TAP(sqlQuery);
			PigLoadCmd loadCmd = (PigLoadCmd)pigScript.getLastCommand();		
		
			//Set Delimiter for the Temporary file created by Sqoop.
			String staging_delimiter = getOptionValue("STAGING_FILE_DELIMITER");
			if( staging_delimiter == null || staging_delimiter.trim().equals("") ) {
				loadCmd.setLoadFuncArgs("'\\t'");  //LKM uses this to read the staging file.
				staging_delimiter = "'\\t'";   //Sqoop uses this to create the staging file.
			}			
			else{
				loadCmd.setLoadFuncArgs(staging_delimiter);
			}
			mBinding.setVariable( "staging_file_delim", staging_delimiter );    
			
			List<MapPhysicalNode> childNodes = physicalNode.getDownstreamConnectedNodes();
			MapPhysicalNode childNode = childNodes.get(0);
			String hdfs_url = childNode.getExecutionUnit().getLocation().getPhysicalSchema().getDataServer().getConnectionInfo().getJdbcUrl();
			mBinding.setVariable( "stagingLocationUrl",  hdfs_url);

			String fileLoc = hdfs_url + "<\$=stagingDir\$>";
			loadCmd.setFileLoc(fileLoc);

			def srcDataServer  = physicalNode.getAPNodeSourceExUnit().getLocation().getDataServer();

			mBinding.setVariable( "srcJavaUrl",    srcDataServer.getConnectionSettings().getJdbcUrl() )
			mBinding.setVariable( "srcUserName",   srcDataServer.getUsername() )
					
			//TODO: To be removed/replaced for security reasons
			def tmpPass     = srcDataServer.getPassword()
			def encodedPass = com.sunopsis.tools.core.SnpsStringTools.replaceNVL(tmpPass.toString())
			
			def generatorContext =  mBinding.getVariable("generatorContext");
			def odiInstance = generatorContext.getObject("odiInstance")
			def clearPass   = "Password decyphering failed!"

			try {
	    			// TODO: For some reason the line below fails when validating a physical mapping
	    			clearPass   = com.sunopsis.dwg.DwgObject.snpsDecypher(encodedPass,odiInstance)
			} catch ( Throwable e ) {}
	
			mBinding.setVariable( "srcPassword", clearPass )
				
			def sqoopQueryString = sqlQuery.getText();
 			sqoopQueryString = sqoopQueryString.replaceAll( "\n", java.util.regex.Matcher.quoteReplacement(" \\\n") );
                	sqoopQueryString += " \\";

			mBinding.setVariable( "sqoopQueryString", sqoopQueryString );
	
			return pigScript;	
		}
       	}
      
   ]]></Field>
