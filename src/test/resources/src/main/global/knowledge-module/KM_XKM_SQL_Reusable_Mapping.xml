	  //}
	  return result
	}
	
	mBinding.setVariable("selectExprList", selectExprList); 
  }
	
  public XKM_SQL_Reusable_Mapping(){
  }
  
	AbstractSyntaxTree SELECT(SqlQuery inputQuery) {
