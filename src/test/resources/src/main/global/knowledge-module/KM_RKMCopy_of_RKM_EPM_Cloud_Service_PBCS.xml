 <Field name="Txt" type="java.lang.String"><![CDATA[import org.json.JSONObject as JSONObject
import xml.etree.ElementTree as ElementTree

#PBCS Connection Details.
Username = "<%=odiRef.getInfo("SRC_USER_NAME")%>"
Password = "<%=odiRef.getInfo("SRC_PASS")%>"
EPMApplicationType = "<%=odiRef.getInfo("SRC_CATALOG")%>"
HPLBaseURL = "<%=odiRef.getInfo("SRC_DSERV_NAME")%>/HyperionPlanning/rest/v3/"
HPLAppName = "<%=odiRef.getInfo("SRC_SCHEMA")%>"
if EPMApplicationType <> "PBCS":
	raise "RKM Error","This RKM is supported only for PBCS/EPBCS type of EPM Cloud applications. Please use 'PBCS' as Application Type in the Physical Schema."

#Generic Function to invoke a RESTful webservice using Basic authentication & return the JSON response.
global invokeRESTService
def invokeRESTService(ServiceURL,Userame,Password,Method,HeaderArray,Payload):
	import urllib,urllib2,base64
	base64Password = base64.encodestring("%s:%s"%(Userame,Password))[:-1]
	RESTRequest = urllib2.Request(ServiceURL)
	RESTRequest.add_header("Authorization","Basic %s" % base64Password)
	for header in HeaderArray:
		RESTRequest.add_header(header[0],header[1])
	RESTRequest.get_method = lambda: Method
	try:
		httpRequest = urllib2.urlopen(RESTRequest,Payload)
		jsonResponse = httpRequest.read()
		httpRequest.close()
		return JSONObject(jsonResponse)
	except urllib2.HTTPError,e:
		if e.code in [201,400]:
			return JSONObject(e.read())
		else:
			raise "RESTAPIInvokeError",str(e.code) + " - " + e.read()

#Generic Function to invoke a RESTful webservice using Basic authentication & return the XML response.
global invokeRESTServiceXML
def invokeRESTServiceXML(ServiceURL,Userame,Password,Method,HeaderArray,Payload):
	import urllib,urllib2,base64
	base64Password = base64.encodestring("%s:%s"%(Userame,Password))[:-1]
	RESTRequest = urllib2.Request(ServiceURL)
	RESTRequest.add_header("Authorization","Basic %s" % base64Password)
	for header in HeaderArray:
		RESTRequest.add_header(header[0],header[1])
	RESTRequest.get_method = lambda: Method
	try:
		httpRequest = urllib2.urlopen(RESTRequest,Payload)
		xmlResponse = httpRequest.read()
		httpRequest.close()
		return ElementTree.fromstring(xmlResponse)
	except urllib2.HTTPError,e:
		if e.code in [201,400]:
			return ElementTree.fromstring(e.read())
		else:
			raise "RESTAPIXMLInvokeError",str(e.code) + " - " + e.read()
			

#Function to Retrieve List of PBCS Applications
def getHPLApplications(HPLBaseURL,Username,Password):
	ServiceURL = HPLBaseURL + 'applications'
	ServiceHeader = []
	ServiceHeader.append(["Content-Type","application/json"])
	ServiceHeader.append(["Accept","application/json"])
	appsResponse = invokeRESTService(ServiceURL,Username,Password,"GET",ServiceHeader,None)
	appList = []
	try:
		appArray = appsResponse.get("items")
		for i in range(0,appArray.length()):
			if appArray.get(i).get("type") == "HP":
				appList.append(str(appArray.get(i).get("name")))
		return appList
	except:
		raise "getHPLApplications",str(appsResponse)

#Function to Retrieve List of PlanTypes for a PBCS application.
def getHPLPlanTypes(HPLBaseURL,Username,Password,HPLAppName):
	ServiceURL = HPLBaseURL + 'internal/applications/' + HPLAppName + '/legacymetadata'
	ServiceHeader = []
	ServiceHeader.append(["Content-Type","application/json"])
	ServiceHeader.append(["Accept","text/xml"])
	xmlRoot = invokeRESTServiceXML(ServiceURL,Username,Password,"GET",ServiceHeader,None)
	xmlplanTypes = xmlRoot.find("plantypes")
	HPLPlanTypes = []
	for p in xmlplanTypes:
		HPLPlanTypes.append(p.get("name"))
	return HPLPlanTypes
	
#Function to Retrieve List of Dimensions for a given PlanType of PBCS application.
def getHPLDimensions(HPLBaseURL,Username,Password,HPLAppName,PlanType):
	ServiceURL = HPLBaseURL + 'internal/applications/' + HPLAppName + '/legacymetadata'
	ServiceHeader = []
	ServiceHeader.append(["Content-Type","application/json"])
	ServiceHeader.append(["Accept","text/xml"])
	xmlRoot = invokeRESTServiceXML(ServiceURL,Username,Password,"GET",ServiceHeader,None)
	xmlplanTypes = xmlRoot.find("plantypes")
	hplDimensions = []
	for p in xmlplanTypes:
		if p.get("name") == PlanType:
			xmlDimensions = p.find("dimensions").findall("dimension")
			for d in xmlDimensions:
				if d.get("type") <> "Attribute":
					hplDimensions.append(d.get("name"))
	return hplDimensions
	
#Fetch List of Apps from PBCS & validate application name.
HPLApps = getHPLApplications(HPLBaseURL,Username,Password)
if HPLAppName not in HPLApps:
	raise "<%=odiRef.getInfo("SRC_SCHEMA")%> application is not found in the PBCS/EPBCS instance. Enter valid application name."
	
#Function to populate datastore names into ODI staging table for reverse engineering.
def Populate_Model_DataStore(repoConnection,DataStoreName,ResourceName,AliasName,Description):
	Statement = repoConnection.createStatement()
	ODI_Tab_Query = """insert into SNP_REV_TABLE(MOD_GUID,TABLE_NAME,RES_NAME,TABLE_ALIAS,TABLE_TYPE,TABLE_DESC,IND_SHOW,R_COUNT,PARTITION_METH,SUB_PARTITION_METH)
	VALUES ('<%=odiRef.getModel("GLOBAL_ID")%>','%s','%s','%s','T','%s','1',0,NULL,NULL)""" % (DataStoreName,ResourceName,AliasName,Description)
	Statement.execute(ODI_Tab_Query)
	Statement.close()
	
#Function to populate datastore column names into ODI staging table for reverse engineering.
def Populate_Model_DataColumns(repoConnection,DataStoreName,ColumnName,DataType,ColumnHeading,ColumnDescription,Position,Length,Scale,MandatoryFlag):
	Statement = repoConnection.createStatement()
	ODI_Col_Query = """insert into SNP_REV_COL(MOD_GUID,TABLE_NAME,COL_NAME,DT_DRIVER,COL_HEADING,COL_DESC,POS,LONGC,SCALEC,COL_MANDATORY,CHECK_STAT,CHECK_FLOW)
	VALUES ('<%=odiRef.getModel("GLOBAL_ID")%>','%s','%s','%s','%s','%s','%d','%d','%d','%d','1','1')""" % (DataStoreName,ColumnName,DataType,ColumnHeading,ColumnDescription,Position,Length,Scale,MandatoryFlag)
	Statement.execute(ODI_Col_Query)
	Statement.close()

#Reverse DataStore for the PBCS application to load data.
modelConnection = odiRef.getJDBCConnection("DEST")

#Fetch List of PlanTypes from PBCS application
HPLPlanTypes = getHPLPlanTypes(HPLBaseURL,Username,Password,HPLAppName)
for p in HPLPlanTypes:
	Populate_Model_DataStore(modelConnection,HPLAppName + '_' + p + '_Data',HPLAppName + '_' + p + '_Data',HPLAppName + '_' + p + '_Data','ODI DataStore for loading data into %s Plan Type' % (p))
	HPLDimensions = getHPLDimensions(HPLBaseURL,Username,Password,HPLAppName,p)
	for d in range(0,len(HPLDimensions)):
		Populate_Model_DataColumns(modelConnection,HPLAppName + '_' + p + '_Data',HPLDimensions[d],'VARCHAR',HPLDimensions[d],HPLDimensions[d],d + 1,80,0,1)
	Populate_Model_DataColumns(modelConnection,HPLAppName + '_' + p + '_Data','DataAmount','NUMBER','DataAmount','DataAmount', d + 2,15,6,0)

#Close Connection
modelConnection.close()]]></Field>
