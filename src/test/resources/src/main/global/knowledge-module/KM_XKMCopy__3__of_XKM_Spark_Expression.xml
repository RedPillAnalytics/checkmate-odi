	<Field name="DelegateScript" type="java.lang.String"><![CDATA[
  	import oracle.odi.mapping.generation.AbstractSyntaxTree.ASTProducerMethod;
	import oracle.odi.domain.mapping.generator.KMGeneratorDelegateRegistry.GeneratorDelegateClass;
  
	@GeneratorDelegateClass(componentType=AggregateComponent.COMPONENT_TYPE_NAME, descriptionKey="GeneratorDelegateClass.SparkExpressionCompKM")
	public class XKM_Spark_Expression extends SparkKM {

		@ASTProducerMethod(processingType=ProcessingType.SOURCE,producedASTType=SparkScript.class, styleKey="ProcessingStyleKey.SparkExpression.SELECT")
		SparkScript SELECT(SparkScript sparkScript) {
		
			supportDataFrames('XKM Spark Expression', true);

			List<SparkScript> sparkScripts = new ArrayList<SparkScript>(1);
			sparkScripts.add(sparkScript);
		
			SparkExpressionCmd expressionCmd = new SparkExpressionCmd(physicalNode.getTemplateUtils(), physicalNode);
			
			List<oracle.odi.domain.mapping.expression.MapExpression> expressions = new ArrayList<oracle.odi.domain.mapping.expression.MapExpression>(2);
			List<String> targetAliases = new ArrayList<String>(1);
			
			for (MapPhysicalExpression physicalExpression : physicalNode.getSourcePhysicalExpressions()) {
			  expressions.add(physicalExpression.getRefLogicalExpression());
			  MapAttribute attribute = physicalExpression.getGeneratedLogicalAttribute();
			  targetAliases.add(attribute.getName());
			}
			
			expressionCmd.setTgtAttrExprs(expressions);
			expressionCmd.setTgtAttrNames(targetAliases);
			
			expressionCmd.setUseSQLExpressions(getOptionValue("SQL_EXPRESSIONS").equalsIgnoreCase("true") ? true : false)
		
			SparkScript expressionScript = new SparkScript(sparkScript.getTemplateUtils(), sparkScripts);
			expressionScript.addChild(expressionCmd);
			
		
		    return expressionScript;
		}
	
	}

  ]]></Field>
