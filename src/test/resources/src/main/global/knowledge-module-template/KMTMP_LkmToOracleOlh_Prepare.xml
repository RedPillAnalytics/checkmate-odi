  odiOlhFiles = odiRef.getDataServerProperty("OLH_FILES", "SRC");
	if (odiOlhFiles == null || odiOlhFiles.length() == 0 ) { odiOlhFiles = ""; } else {
        odiOlhFiles = "-files " + odiOlhFiles; };
	
