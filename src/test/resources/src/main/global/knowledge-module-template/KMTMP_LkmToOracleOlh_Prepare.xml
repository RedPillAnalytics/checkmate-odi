	if (odiOlhJars == null || odiOlhJars.length() == 0 ) { odiOlhJars = ""; /* throw new RuntimeException( ""+(char)10+(char)10+(char)10+"Required unix environment variable ODI_OLH_JARS is undefined!"+(char)10+(char)10+(char)10); */ } else {
