    if not rdd.isEmpty():
      print("Some rows")
      {# IF ($[SPARK_STREAMING_DEBUG] == 'true') #}
      for line in rdd.collect():
        print line.encode('utf8')
      {# ENDIF #}
      try:
        rdd.saveAsTextFile(path + '-' + t.strftime('%Y%m%d%H%M%S%f'))
      except Py4JJavaError as e:
        if 'FileAlreadyExistsException' not in str(e):raise
    else:
      print("No rows")
  return saveFunc]]></Field>
